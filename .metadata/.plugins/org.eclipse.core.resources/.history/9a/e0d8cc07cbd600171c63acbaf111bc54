
public class YourSchedule 
{
	public static void main(String[] args)
		{
		String class1, class2, class3, class4, teacher1, teacher2, teacher3, teacher4;
		
		class1= "AP Calculus AB";
		class2= "Computer Science";
		class3= "College Apps Seminar";
		class4= "AP Biology";
		teacher1= "Farrar";
		teacher2= "Mauro";
		teacher3= "Black";
		teacher4= "Gerstin";
		
		System.out.println("Keana's Class Schedule:");
		System.out.println("");
		System.out.println("-----------------------------------------------------");
		System.out.println("| Per 1 |         " + class1 + "                       |           " + teacher1 + "             |");
		System.out.println("| Per 2 |         " + class2 + "                 |           " + teacher2 + "             |");
		System.out.println("| Per 3 |         " + class3 + "           |           " + teacher3 + "             |");
		System.out.println("| Per 4 |         " + class4 + "                             |           " + teacher4 + "             |");
		System.out.println("-----------------------------------------------------");
		
		}
}
