import java.util.Scanner;

public class MethodPass 
{
	public static void main(String[] args)
	{

		
		int answer;
		boolean response;
		
		answer = check();

		if (response)
			System.out.print("");
		else
			System.out.print("");
		
		
	}
	
	public static boolean check()
	{
		Scanner keyboard = new Scanner(System.in);
		
		int input;
		boolean response;
		
		System.out.print("Pick a number: " );
		input = keyboard.nextInt();
		
		if( input % 3 ==0)
			response = true;
		else 
			response = false;
			
		
		return response;
				
		
	}
}
